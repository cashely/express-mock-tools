{
    "version": 2,
    "devCommand": "pnpm run dev",
    "buildCommand": "pnpm run build-swc",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "builds": [
        {
            "src": "./dist/index.js",
            "use": "@vercel/node",
            "config": {
                "zeroConfig": true
            }
        }
    ]
}