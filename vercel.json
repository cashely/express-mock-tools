{
    "version": 2,
    "devCommand": "pnpm run dev",
    "buildCommand": "pnpm run build-swc",
    "routes": [
    {
        "src": "/(.*)",
        "dest": "/api"
    }
    ],
    "builds": [
        {
            "src": "./dist/api/index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["./dist/**"]
            }
        }
    ]
}