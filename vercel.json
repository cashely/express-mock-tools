{
    "version": 2,
    "devCommand": "pnpm run dev",
    "buildCommand": "pnpm run build-swc",
    "builds": [
        {
            "src": "./dist/index.js",
            "use": "@vercel/node",
            "config": {
                "zeroConfig": true
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "./index.js",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "PATCH",
                "OPTIONS"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}